{  
  "extends": [
     "tslint:recommended",
   
    "tslint-config-prettier"
  ],
  "rules": {
    "no-var-keyword": [false],
    "no-inferrable-types" : [true],
    "directive-selector": [
      true,
      "attribute",
      "app",
      "camelCase"
    ],
    "component-selector": [
      true,
      "element",
      "app",
      "kebab-case"
    ]
   
  }
}
